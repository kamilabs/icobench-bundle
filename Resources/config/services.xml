<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="kami.icobench.client" class="Kami\IcoBench\Client" public="true">
            <argument>%kami.icobench.private_key%</argument>
            <argument>%kami.icobench.public_key%</argument>
            <argument>%kami.icobench.guzzle_options%</argument>
        </service>
    </services>

</container>
